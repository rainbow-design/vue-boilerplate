{
  "env": {
    "es6": true,
    "commonjs": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "parser": "babel-eslint",
    "sourceType": "module"

  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "quotes": "off",
    "vue/no-unused-components": 1 // 警告就行了
  }
}
