{
  "name": "rainbow-vue-cli",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [
    "vue",
    "cli",
    "webpack"
  ],
  "author": "yanyue404",
  "license": "ISC",
  "scripts": {
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "lint": "eslint --ext .js --ext .vue src/",
    "lint:fix": "eslint --fix --ext .js --ext .vue src/",
    "analyz": "webpack-bundle-analyzer --port 8888 ./dist/stats.json",
    "build:analyz": "npm run build && npm run analyz ",
    "prettier": "prettier --write \"./src/*.{js,jsx,vue,css,html,scss,md,json}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,vue,css,html,scss,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "husky": "^4.0.10",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.1",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
